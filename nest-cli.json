{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/microservice-calculator/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/microservice-calculator/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/microservice-calculator",
  "projects": {
    "microservice-calculator": {
      "type": "application",
      "root": "apps/microservice-calculator",
      "entryFile": "main",
      "sourceRoot": "apps/microservice-calculator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/microservice-calculator/tsconfig.app.json"
      }
    },
    "parity": {
      "type": "application",
      "root": "apps/parity",
      "entryFile": "main",
      "sourceRoot": "apps/parity/src",
      "compilerOptions": {
        "tsConfigPath": "apps/parity/tsconfig.app.json"
      }
    },
    "prime": {
      "type": "application",
      "root": "apps/prime",
      "entryFile": "main",
      "sourceRoot": "apps/prime/src",
      "compilerOptions": {
        "tsConfigPath": "apps/prime/tsconfig.app.json"
      }
    }
  }
}